                        card.addEventListener('mousedown', (e) => {
                            isDragging = true;
                            initialX = e.clientX;
                            initialY = e.clientY;
                        });

                        document.addEventListener('mousemove', (e) => {
                            if (!isDragging) return;
                            currentX = e.clientX - initialX;
                            currentY = e.clientY - initialY;

                            card.style.transform = `translate(${currentX}px, ${currentY}px)`;
