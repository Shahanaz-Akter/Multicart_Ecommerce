                <form action="/post_review/<%=details_secondary_img.id%>" method="post" class="mb-3" enctype="multipart/form-data">
                  <%let len=reviews.length%>
                    <h6 class="col-xl-6 fw-bold mb-3">Customer Reviews (<%=len%>)</h6>
                    <!-- col-12 col-md-6 col-lg-6 col-xl-6 -->
                    <div class="row">
                      <span class="col-6 col-md-4 col-lg-4 col-xl-4 mb-3">
